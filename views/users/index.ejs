<h2>Users</h2>

<table class="table">
  <thead>
    <tr>
      <th>Username</th>
      <th>Password</th>
    </tr>
  </thead>
  <tbody>
    <% for (let user of users) { %>
      <tr>
        <td>
          <a href="/users/<%= user.id %>" class="btn btn-xs"><%= user.username %></a>
        </td>
        <td><%= user.password %></td>
      </tr>
    <% } %>
  </tbody>
</table>
